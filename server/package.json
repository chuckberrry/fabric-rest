    "express-session": "^1.15.6",
    "fabric-ca-client": "^1.2.1",
    "fabric-client": "^1.2.1",
    "fs-extra": "^2.0.0",
    "jsonwebtoken": "^7.3.0",
    "log4js": "^0.6.38",
